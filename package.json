{
  "name": "styleguide",
  "version": "0.0.0",
  "homepage": "https://vtex.github.io/styleguide",
  "private": true,
  "workspaces": [
    "packages/*",
    "packages/components/*"
  ],
  "scripts": {
    "storybook:start": "start-storybook -p 6006",
    "storybook:build": "build-storybook",
    "clean": "lerna clean",
    "bootstrap": "lerna bootstrap",
    "lint": "eslint --quiet --fix packages/components/*/src/**/*.js",
    "postinstall": "yarn run bootstrap"
  },
  "dependencies": {
    "@babel/cli": "^7.6.0",
    "@babel/core": "^7.5.4",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-modules-commonjs": "^7.0.0",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@storybook/addon-a11y": "^5.1.11",
    "@storybook/addon-actions": "^5.1.11",
    "@storybook/addon-info": "^5.1.11",
    "@storybook/addon-knobs": "^5.1.11",
    "@storybook/addon-links": "^5.1.11",
    "@storybook/addon-viewport": "^5.1.11",
    "@storybook/addons": "^5.1.11",
    "@storybook/cli": "^5.1.11",
    "@storybook/react": "^5.1.11",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^8.0.6",
    "babel-plugin-dynamic-import-node": "^1.2.0",
    "eslint": "6",
    "eslint-config-vtex": "^11.0.0",
    "eslint-config-vtex-react": "^5.0.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-lodash": "^6.0.0",
    "lerna": "^3.16.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.18.2",
    "prop-types": "^15.7.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "typescript": "^3.5.3",
    "vtex-tachyons": "^3.2.0"
  }
}
